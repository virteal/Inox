{
        "version": "0.2.0",
        "configurations": [
                
                {
                        "name": "Launch node inox.js",
                        "type": "node",
                        "request": "launch",
                        "program": "${workspaceFolder}\\builds\\inox.js",
                        "skipFiles": [
                                "<node_internals>/**"
                        ],
                        "outputCapture": "std"
                },
                {
                        "name": "C++: debug inox.exe",
                        "type": "cppvsdbg",
                        "request": "launch",
                        "program": "${workspaceFolder}\\builds\\inox.exe",
                        "args": [],
                        "stopAtEntry": false,
                        "cwd": "${workspaceFolder}",
                        "environment": [],
                        "console": "integratedTerminal"
                }
        ]
}
